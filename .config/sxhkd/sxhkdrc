super + shift + r
  pkill sxhkd && sxhkd &

super + shift + i 
  telegram-desktop

super + u
  mount.sh

super + w
  zen-browser

super + o
  cpick.sh

super + f 
  kitty -1 --class fileManager -e yazi

Print 
  flameshot gui --clipboard

shift + Print 
  flameshot full --clipboard

super + {equal, minus, BackSpace}
  volume.sh {up, down, toggle}

super + shift + BackSpace
  exit.sh
