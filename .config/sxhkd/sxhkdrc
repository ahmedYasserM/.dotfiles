super + shift + r
  pkill sxhkd && sxhkd &

super + shift + i 
  telegram-desktop

super + u
  mount.sh

super + w
  zen-browser

super + shift + n
  cpick.sh

super + f 
  kitty -1 -e yazi

super + s 
  flameshot gui

super + shift + s
  flameshot full --clipboard

super + {equal, minus, BackSpace}
  volume.sh {up, down, toggle}

super + shift + BackSpace
  exit.sh
